<html>
	<head>
		<title>{{ APP_TITLE }} | {% block title %}{% endblock %}</title>
	</head>
	<body>
		<h1>{{ APP_TITLE }}</h1>
		<h2>{% block heading %}{% endblock %}</h2>
		<ul class="main_menu">
			{% for menu_item in main_menu_items %}
			<li><a href="{{ menu_item.href }}">{{ menu_item.title }}</a></li>
			{% endfor %}
		</ul>
		{% block body %}
		Default Body
		{% endblock %}
	</body>
</html> 